{
  "editor.renderLineHighlight": "none",
  "editor.tabSize": 2,
  "editor.renderIndentGuides": false,
  "editor.matchBrackets": false,
  "editor.occurrencesHighlight": true,
  "editor.cursorStyle": "line",
  "editor.cursorBlinking": "solid",
  "editor.lineHeight": 21,
  "editor.fontSize": 13,
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "workbench.colorTheme": "CloudsMidnight",
  "workbench.activityBar.visible": true,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.statusBar.feedback.visible": false,
  "workbench.sideBar.location": "right",
  "workbench.colorCustomizations": {
    "editor.wordHighlightBackground": "#1b1b1b",
    "editor.wordHighlightStrongBackground": "#1b1b1b",
    "editor.wordHighlightBorder": "#3a3a3a",
    "editor.wordHighlightStrongBorder": "#3a3a3a"
  },
  "window.menuBarVisibility": "default",
  "window.zoomLevel": 0,
  "files.autoSave": "off",
  "files.insertFinalNewline": true,
  "terminal.integrated.shell.windows": "D:\\_\\tools\\Git\\bin\\bash.exe",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.lineHeight": 1.1,
  "bracketPairColorizer.forceUniqueOpeningColor": false,
  "bracketPairColorizer.showVerticalScopeLine": false,
  "bracketPairColorizer.scopeLineCSS": [
    "borderStyle : solid",
    "borderWidth : 1px",
    "borderColor : {color}; opacity: 0.10"
  ],
  "bracketPairColorizer.showBracketsInGutter": true,
  "bracketPairColorizer.consecutivePairColors": [
    [
      "{",
      "}"
    ],
    [
      "#c68b25",
      "#77a53e",
      "#3d96a5"
    ],
    "White"
  ]
}
